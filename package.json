    "concurrently": "5.2.0",
    "cors": "2.8.5",
    "date-fns": "2.12.0",
    "dinero.js": "1.8.1",
    "dotenv": "8.2.0",
    "express-paginate": "1.0.0",
    "express-session": "1.17.1",
    "express-validator": "6.4.1",
    "faker": "4.1.0",
    "formik": "2.1.4",
    "fuse.js": "5.2.3",
    "husky": "4.2.5",
    "json": "9.0.6",
    "lowdb": "1.0.0",
    "morgan": "1.10.0",
    "nodemon": "2.0.3",
